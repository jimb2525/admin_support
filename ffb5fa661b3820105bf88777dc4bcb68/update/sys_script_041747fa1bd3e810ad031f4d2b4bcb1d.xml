        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
	var preferences =
		new GlideRecord('x_paha_admin_suppo_special_handling_preference');

	preferences.addQuery('user', current.u_job_requestor);
	preferences.addQuery('active', true);
	preferences.query();
	
	var detail =
		new GlideRecord('x_paha_admin_suppo_special_handling_detail');
	
	while (preferences.next()) {
		detail.initialize();
		detail.parent = current.sys_id;
		detail.message = preferences.message;
		detail.insert();
	}

})(current, previous);]]></script>
