   if (newValue == '' || newValue == null) {
      g_form.setValue('location', '');
      return;
   }
   if (!g_form.hasField('location'))
      return;
   var openedBy = g_form.getReference('opened_by', setAssignmentGroup);
}

function setAssignmentGroup(openedBy) {
   if (openedBy) 
       g_form.setValue('assignment_group', openedBy.assignmentGroup);
}
]]></script>
